<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bainfogame">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bainfogame"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".about.aboutcr"
            android:exported="true" />
        <activity
            android:name=".bguide.bguide"
            android:exported="true"
            android:label="Beginner Guide"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".equipment.EqActivity"
            android:exported="true"
            android:label="Equipment"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".komen.editkmn"
            android:exported="true"
            android:parentActivityName=".komen.komenFeed" />
        <activity
            android:name=".komen.reg_kmn"
            android:exported="true"
            android:parentActivityName=".komen.komenFeed" />
        <activity
            android:name=".komen.log_kmn"
            android:exported="true"
            android:parentActivityName=".komen.komenFeed" />
        <activity
            android:name=".komen.addkmn"
            android:exported="true"
            android:parentActivityName=".komen.komenFeed" />
        <activity
            android:name=".komen.komenFeed"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".student.StudentList"
            android:label="Student Lists"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>